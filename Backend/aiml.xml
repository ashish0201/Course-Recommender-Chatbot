<?xml version="1.0" encoding="UTF-8"?>
<aiml>
    <category>
        <pattern>*</pattern>
        <template>I have no answer for that.</template>
    </category>
    <category>
        <pattern>HI MY NAME IS *</pattern>
        <template>Hi <set name = "firstname"><star/></set>, how are you?</template>
    </category>
